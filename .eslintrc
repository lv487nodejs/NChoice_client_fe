{
    "extends": [
        "wesbos"
    ],
    "rules": {
        "no-console": 0,
        "no-use-before-define": 0,
        "no-throw-literal": 0,
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "es5",
                "singleQuote": true,
                "printWidth": 120,
                "tabWidth": 4
            }
        ]
    }
}